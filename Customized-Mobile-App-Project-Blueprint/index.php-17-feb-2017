       // $("#nav-mobile").html($("#nav-main").html());
        jQuery("#nav-trigger").click(function(){
            if (jQuery("nav#main-nav ul").hasClass("expanded")) {
                jQuery("nav#main-nav ul.expanded").removeClass("expanded").slideUp(250);
                jQuery("nav#main-nav").removeClass("open");
				jQuery("nav#main-nav").addClass("close");
            } else {
                jQuery("nav#main-nav ul").addClass("expanded").slideDown(250);
                jQuery("nav#main-nav").addClass("open");
				jQuery("nav#main-nav").removeClass("close");
            }
        });
 });</script>
 
 

 
 
